    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "body-parser": "1.20.2",
    "chalk": "4.1.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "express": "4.18.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.9"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prettier": "2.1.2"
